// Base light theme template
// Placeholders: {{role}} resolves to brand colour, {{role}}XX appends hex alpha
{
  "type": "light",
  "colors": {
    "editor.background": "{{background}}",
    "editor.foreground": "{{foreground}}",
    "editorCursor.foreground": "{{accent}}",
    "editor.lineHighlightBackground": "{{backgroundDeep}}",
    "editor.selectionBackground": "{{accent}}30",
    "editor.selectionHighlightBackground": "{{accent}}20",
